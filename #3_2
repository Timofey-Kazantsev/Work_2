using System;

namespace массив_3_2
{
    class Program
    {
        static void Main(string[] args)
        {
            int[] t = new int[8] { 5, 11, 25, 12, 9, 2, 70, 228 };
            for (int i =0; i<t.Length; i++)
            {
                if (t[i] < 15)
                {
                    t[i] =t[i] * 2;
                }
                Console.Write(t[i] + " ");
            }
            Console.ReadKey();
        }
    }
}
